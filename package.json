{
    "name": "template-builder",
    "version": "0.0.1",
    "description": "Template Builder for Pages",
    "scripts": {
        "start": "npm run watch-and-preview",
        "clean": "rimraf /dist",
        "watch": "npm run clean && node scripts/watchTemplateChanges.js",
        "watch-and-preview": "npm run clean && node scripts/watchAndPreview.js",
        "build-templates": "npm run clean && node scripts/buildTemplates.js",
        "preview-templates": "npm run clean && node scripts/previewTemplates.js"
    },
    "devDependencies": {
        "express": "^4.14.1",
        "nunjucks": "^3.0.0",
        "rimraf": "^2.6.1"
    }
}
